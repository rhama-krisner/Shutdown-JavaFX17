<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<AnchorPane prefHeight="150.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.desligaautomatico.ViewController">
   <children>
      <GridPane layoutY="27.0" prefHeight="37.0" prefWidth="297.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ComboBox fx:id="cbHora" prefHeight="25.0" prefWidth="102.0" />
            <ComboBox fx:id="cbMinuto" prefHeight="25.0" prefWidth="102.0" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label layoutX="25.0" layoutY="19.0" text="Horas" />
      <Label layoutX="173.0" layoutY="19.0" text="Minutos" />
      <Button fx:id="btnDesligar" layoutX="25.0" layoutY="82.0" mnemonicParsing="false" onAction="#BtnDesligarOnClick" prefHeight="25.0" prefWidth="70.0" text="Desligar" />
      <Button fx:id="btnReiniciar" layoutX="114.0" layoutY="82.0" mnemonicParsing="false" onAction="#BtnReiniciarOnClick" prefHeight="25.0" prefWidth="70.0" text="Reiniciar" />
      <Button fx:id="btnCancelar" layoutX="203.0" layoutY="82.0" mnemonicParsing="false" onAction="#BtnCancelarOnClick" prefHeight="25.0" prefWidth="70.0" text="Cancelar" />
   </children>
</AnchorPane>
